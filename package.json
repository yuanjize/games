{
  "name": "web-games-collection",
  "version": "1.0.0",
  "description": "Collection of 10 web games built with HTML, CSS and JavaScript",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "build": "bash build.sh",
    "build:vite": "vite build",
    "dev": "npx wrangler dev",
    "dev:vite": "vite",
    "preview": "vite preview",
    "deploy": "npm run build && npx wrangler deploy --assets=./dist",
    "deploy:production": "npm run build && npx wrangler deploy --env production --assets=./dist",
    "deploy:vite": "npm run build:vite && npx wrangler deploy",
    "tail": "npx wrangler tail",
    "lint": "eslint scripts tests *.js",
    "lint:fix": "eslint scripts tests *.js --fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "analyze": "vite-bundle-visualizer"
  },
  "keywords": [
    "games",
    "web-games",
    "javascript-games",
    "cloudflare-workers"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "jsdom": "^27.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.36.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.16",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "puppeteer": "^24.34.0"
  }
}
